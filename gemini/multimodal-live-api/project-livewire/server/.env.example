# Project Configuration
PROJECT_ID=your-project-id
LOG_LEVEL=INFO

# API Configuration
VERTEX_API=true  # Set to true to use Vertex AI endpoint
VERTEX_LOCATION=us-central1  # Required when VERTEX_API=true

# Model Configuration
MODEL_DEV_API=models/gemini-2.0-flash-exp
MODEL_VERTEX_API=gemini-2.0-flash-exp
VOICE_DEV_API=Puck
VOICE_VERTEX_API=Aoede

# Cloud Function URLs
WEATHER_FUNCTION_URL=https://REGION-PROJECT_ID.cloudfunctions.net/get-weather-tool
CALENDAR_FUNCTION_URL=https://REGION-PROJECT_ID.cloudfunctions.net/get-calendar-tool

# API Keys
GOOGLE_API_KEY=your_google_api_key  # Required for dev endpoint (VERTEX_API=false)
OPENWEATHER_API_KEY=your_openweather_api_key  # Required for weather functions
