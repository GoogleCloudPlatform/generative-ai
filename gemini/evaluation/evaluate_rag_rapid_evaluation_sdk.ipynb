    "rag_eval_task_model_a = EvalTask(\n",
    "    dataset=eval_dataset_model_a,\n",
    "    metrics=[\n",
    "        \"question_answering_quality\",\n",
    "        relevance,\n",
    "        helpfulness,\n",
    "        \"groundedness\",\n",
