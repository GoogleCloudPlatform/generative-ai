export ENVIRONMENT=development
export FRONTEND_URL=http://localhost:4200

# --- Google Cloud Configuration ---
# Required for using Vertex AI
export GOOGLE_GENAI_USE_VERTEXAI=1
export GOOGLE_CLOUD_PROJECT="your-gcp-project-id" # Replace with your Google Cloud Project ID
export GOOGLE_CLOUD_LOCATION="us-central1"      # Replace with your Google Cloud Location (e.g., us-central1)

# --- Model Configuration ---
# Specify the default model to be used by the agents.
# Ensure this model is available in your Vertex AI project and location.
# Examples: "gemini-1.0-pro-001", "gemini-1.5-flash-001", "gemini-1.5-pro-latest"
export DEFAULT_MODEL="gemini-1.5-flash-001"

# --- Optional: Agent Engine Configuration ---
 export AGENT_ENGINE_BUCKET_NAME="your-ae-staging-bucket-name" # Used for deploying to Agent Engine on Vertex AI